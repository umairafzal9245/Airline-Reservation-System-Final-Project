      <TextField fx:id="usernamefield" editable="false" layoutX="201.0" layoutY="196.0" prefHeight="35.0" prefWidth="170.0" promptText="admin" text="admin" />
      <Label layoutX="117.0" layoutY="204.0" prefHeight="20.0" prefWidth="74.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="117.0" layoutY="276.0" prefHeight="20.0" prefWidth="74.0" text="Pin">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="185.0" fitWidth="224.0" layoutX="194.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@avatar.png" />
